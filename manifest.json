{
  "chrome_settings_overrides": {
    "search_provider": {
      "name": "Sepia Search",
      "keyword": "sepia",
      "search_url": "https://sepiasearch.org/search?search={searchTerms}",
      "favicon_url": "https://sepiasearch.org/img/favicon.png",
      "is_default": false
    }
  },
  "manifest_version": 2,
  "name": "Sepia Search",
  "description": "Search on the Fediverse with FramaSoft's Sepia Search.",
  "version": "1.0.1",
  "permissions": [
    "storage",
    "https://sepiasearch.org/*",
    "webRequest",
    "webRequestBlocking",
    "cookies"
  ],
  "applications": {
    "gecko": {
      "id": "@sepia-search",
      "strict_min_version": "57.0"
    }
  }
}
